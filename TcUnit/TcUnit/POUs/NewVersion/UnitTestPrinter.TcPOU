    <Method Name="getFailureMessage" Id="{9dc6d79a-5505-4a87-87c3-d856af6043df}">
      <Declaration><![CDATA[METHOD getFailureMessage : MaxString
VAR_INPUT
    expected: ANY;
    actual: ANY;
    message: MaxString;
    path: MaxString;
END_VAR
VAR
    messageToAppend: MaxString;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[messageToAppend := 'FAILED TEST $'';
messageBuilder.append(messageToAppend);
messageBuilder.append(path);
messageToAppend := '$', EXP: ';
messageBuilder.append(messageToAppend);
messageBuilder.append(expected);
messageToAppend := ', ACT: ';
messageBuilder.append(messageToAppend);
messageBuilder.append(actual);
messageToAppend := ', MSG: ';
messageBuilder.append(messageToAppend);
messageBuilder.append(message);

getFailureMessage := messageBuilder.toString();]]></ST>
      </Implementation>
    </Method>
    <Method Name="printFailure" Id="{af4e89be-6949-40ab-9696-5dfe4e50e8cf}">
      <Declaration><![CDATA[METHOD printFailure
VAR_INPUT
    expected: MaxString;
    actual: MaxString;
    message: MaxString;
    path: MaxString;
END_VAR
VAR
    failureMessage: MaxString;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[failureMessage := getFailureMessage(expected, actual, message, path);]]></ST>
      </Implementation>
    </Method>
